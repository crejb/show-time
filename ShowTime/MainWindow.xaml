<Window x:Class="ShowTime.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:viewmodel="clr-namespace:ShowTime.ViewModel"
        xmlns:view="clr-namespace:ShowTime.View"
        xmlns:model="clr-namespace:ShowTime.Model"
        Title="MainWindow" Height="350" Width="525" >
    <Window.Resources>
        <ResourceDictionary Source="MainWindowResources.xaml" />
    </Window.Resources>
    
    <Grid>
        <Border Background="Red" BorderBrush="Blue" BorderThickness="4">
            <StackPanel>
                <view:BrowseAllShowsView x:Name="BrowseAllView"></view:BrowseAllShowsView>
            </StackPanel>
        </Border>
    </Grid>
</Window>
