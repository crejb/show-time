<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewmodel="clr-namespace:ShowTime.ViewModel"
                    xmlns:iconlistviewmodel="clr-namespace:ShowTime.ViewModel.IconListViews"
                    xmlns:view="clr-namespace:ShowTime.View"
                    xmlns:iconlistviews="clr-namespace:ShowTime.View.IconListViews">
    <DataTemplate DataType="{x:Type viewmodel:MainMenuViewModel}">
        <view:MainMenuView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:BrowseAllShowsViewModel}">
        <view:BrowseAllShowsView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:UpdateShowTimeCollectionViewModel}">
        <view:UpdateShowTimeCollectionView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type iconlistviewmodel:TvShowIconListViewModel}">
        <iconlistviews:TvShowIconListView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type iconlistviewmodel:SeasonIconListViewModel}">
        <iconlistviews:SeasonIconListView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type iconlistviewmodel:EpisodeIconListViewModel}">
        <iconlistviews:EpisodeIconListView />
    </DataTemplate>

</ResourceDictionary>