<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewmodel="clr-namespace:ShowTime.ViewModel"
                    xmlns:view="clr-namespace:ShowTime.View">
    <DataTemplate DataType="{x:Type viewmodel:MainMenuViewModel}">
        <view:MainMenuView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:BrowseAllShowsViewModel}">
        <view:BrowseAllShowsView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:UpdateShowTimeCollectionViewModel}">
        <view:UpdateShowTimeCollectionView />
    </DataTemplate>
    
</ResourceDictionary>