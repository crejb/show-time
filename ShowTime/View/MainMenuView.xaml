             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:view="clr-namespace:ShowTime.View"
             xmlns:commands="clr-namespace:ShowTime.ViewModel.Commands"
             xmlns:diagnostics="clr-namespace:System.Diagnostics;assembly=WindowsBase"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <view:IconListPanel MenuItemCommands="{Binding MenuItemCommands, diagnostics:PresentationTraceSources.TraceLevel=High}">
            <!--<view:IconListPanel.IconContent>
                <DataTemplate>
                    <Label Content="{Binding DataContext, diagnostics:PresentationTraceSources.TraceLevel=High}" />
                </DataTemplate>
            </view:IconListPanel.IconContent>-->
        </view:IconListPanel>
    </Grid>
</UserControl>
